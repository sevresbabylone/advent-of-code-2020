{
  "compilerOptions": {
    "moduleResolution": "node",
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "sourceMap": true,
    "outDir": "dist",
    "types": ["node", "jest"],
  },
  "include": ["**/*.ts"]
}
